<Window x:Class="KT_1_3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KT_1_3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100*"/>
            <RowDefinition Height="350*"/>
            <RowDefinition Height="7*"/>
        </Grid.RowDefinitions>
        <Grid Background ="AliceBlue" Panel.ZIndex="-2"></Grid>
        <Image HorizontalAlignment="Left" Height="89" Margin="10,10,0,0" VerticalAlignment="Top" Width="87" Grid.RowSpan="2" Source="/Data/logo.png"/>
        <Label Content="OPERATOR" HorizontalAlignment="Center" Margin="0,23,0,0" VerticalAlignment="Top" HorizontalContentAlignment="Center" FontSize="24" FontWeight="Bold"/>
        <Button Content="Back" HorizontalAlignment="Left" Height="42" Margin="653,0,0,0" VerticalAlignment="Center" Width="121" Background="#FF5DB6EE" FontSize="18" FontWeight="Bold"/>
        <Frame x:Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
